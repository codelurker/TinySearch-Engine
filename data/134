   var isNS6 = (!document.all && document.getElementById) ? true : false;
   var home_bg_index;

   function pickhome_bg() {
      home_bg_index = 9;
      filename = "home_bg" + home_bg_index + ".gif";
      photo = new Image(); photo.src = filename;
      document.home_bg.src = photo.src;
   }

   function setvis( name, val ) {
      if( isNS4 ) {
          p = document.layers[name];
	  (val == 1) ? p.visibility = "show" : p.visibility = "hidden";
      } else {
	if( isIE4 ){
          p = document.all[name].style;
	  ( val == 1 ) ? p.visibility = "visible" : p.visibility = "hidden";
	} else if( isIE5 || isNS6 ) {
          p = document.getElementById(name).style;
	  (val == 1 ) ? p.visibility = "visible" : p.visibility = "hidden";
	} else {
          p = document.getElementById(name).style;
	  ( val == 1 ) ? p.visibility = "show" : p.visibility = "hidden";
	}
      }
   }
